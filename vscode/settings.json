{
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[scss]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[graphql]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "off"
  },
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorStyle": "block",
  "editor.fontFamily": "\"FiraCode-Retina\", 'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.tabSize": 2,
  "extensions.ignoreRecommendations": true,
  "git.confirmSync": false,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.remotes": [{ "domain": "git.target.com", "type": "GitHub" }],
  "highlight.regexes": {
    "(/[/*] ?)(TODO:?|END-TODO:?)(.*)?": [
      {},
      {
        "color": "rgb(255,255,100)",
        "overviewRulerColor": "rgba(255,255,0,0.85)",
        "overviewRulerLane": 4
      },
      {
        "color": "rgb(215,215,0)"
      }
    ],
    "(/[/*] ?)(DEPRECATED)(.*)?": [
      {},
      {
        "color": "rgb(255,100,100)",
        "overviewRulerColor": "rgba(255,100,0,0.85)",
        "overviewRulerLane": 4
      },
      {
        "color": "rgb(215,100,100)"
      }
    ]
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "search.smartCase": true,
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
  "terminal.integrated.rendererType": "dom",
  "typescript.experimental.useSeparateSyntaxServer": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.changeWordIncludesWhitespace": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": [":noh"]
    },
    {
      "before": ["<leader>", "b", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b", "l"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "b", "j"],
      "commands": ["bookmarks.jumpToNext"]
    },
    {
      "before": ["<leader>", "b", "k"],
      "commands": ["bookmarks.jumpToPrevious"]
    }
  ],
  "vim.smartcase": true,
  "workbench.colorTheme": "Monokai",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "github-enterprise.uri": "https://git.target.com/",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "files.insertFinalNewline": true,
  "eslint.runtime": "/Users/Z002R31/.nvm/versions/node/v22.14.0/bin/node",
  "chat.agent.enabled": true
}
